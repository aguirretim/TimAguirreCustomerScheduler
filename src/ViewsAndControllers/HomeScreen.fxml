      </Button>
      <Button fx:id="apptMonthReportButton" layoutX="309.0" layoutY="400.0" mnemonicParsing="false" prefHeight="58.0" prefWidth="89.0" text="Appointment Monthly Report" textAlignment="CENTER" wrapText="true">
         <font>
            <Font size="11.0" />
         </font>
      </Button>
      <Button fx:id="allConsultanReportButton" layoutX="411.0" layoutY="400.0" mnemonicParsing="false" prefHeight="58.0" prefWidth="89.0" text="All Consultants Appoinments Report" textAlignment="CENTER" wrapText="true">
         <font>
            <Font size="11.0" />
         </font>
      </Button>
      <Button fx:id="past30DayButton" layoutX="511.0" layoutY="400.0" mnemonicParsing="false" prefHeight="58.0" prefWidth="89.0" text="Past 30 Day Appointment Report" textAlignment="CENTER" wrapText="true">
         <font>
            <Font size="11.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
