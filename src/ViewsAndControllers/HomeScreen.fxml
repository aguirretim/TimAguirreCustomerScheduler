<AnchorPane id="AnchorPane" prefHeight="487.0" prefWidth="786.0" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ViewsAndControllers.HomeScreenController">
